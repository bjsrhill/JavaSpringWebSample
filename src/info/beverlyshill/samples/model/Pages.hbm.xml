<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="info.beverlyshill.samples.model.Pages" table="Pages">
		<id name="pageId" column="pageId">
			<generator class="identity"/>
		</id>
		<property name="name" type="java.lang.String" column="name" length="255" not-null="true"/>
		<property name="textDesc" type="java.lang.String" column="textDesc" length="255" not-null="true"/>
		<property name="detailPage" type="java.lang.String" column="detailPage" length="255" not-null="false"/>
   </class>
</hibernate-mapping>
